{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "isolatedModules": true,
    "skipLibCheck": true,

    "outDir": "./js-dist",
    "allowJs": true,
    "checkJs": false
  },
  "include": ["js/**/*.ts", "js/**/*.js", "js/**/*.d.ts"],
  "exclude": ["js/api.js"]
}